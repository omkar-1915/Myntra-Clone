pipeline {
    agent any
    stages {
        stage('SCM Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/yashighokar1412/Myntra-Clone.git'
            }
        }
    stage('docker build') {
            steps { 
               sh 'docker build -t yashthedocker/myntra:01 .'
               sh 'docker push yashthedoker/myntra:01'
               sh 'docker images'
                }
            }}}
