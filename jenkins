pipeline {
    agent any
    
    stages {
        stage('SCM Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/yashighokar1412/Myntra-Clone.git'
            }
        }
    stage('Sonar Analysis') {
            steps {
                withSonarQubeEnv(credentialsId: 'sonar', installationName: 'sonarqube') {
                    sh 'mvn clean verify sonar:sonar'
' 
                }
            }
        }
}}
