pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/yashighokar1412/Myntra-Clone.git'
            }
        } 
        stage(docker images') {
            steps {withDockerRegistry(credentialsId: 'docker') {
             sh 'docker build . -t myntra'
}}}}}
