pipeline {
agent any
stages {
      stage('scm checkout'){
            steps{git branch: 'main', url: 'https://github.com/yashighokar1412/Myntra-Clone.git'
}}
       stage('sonar scannar'){
            steps{ withSonarQubeEnv(credentialsId: 'sonar', installationName: 'sonar') {
             sh 'mvn sonar:sonar'
}
}}}}
